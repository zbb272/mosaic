<h1> Who You?</h1>

<%= form_for @user do |u|  %>
  <%= u.label :username %>
  <%= u.text_field :username %><br>
  <br>

  <%= u.label :first_name %>
  <%= u.text_field :first_name %><br>
  <br>

  <%= u.label :last_name %>
  <%= u.text_field :last_name %><br>
  <br>


  <%= u.label :bio %>
  <%= u.text_area :bio %><br>
  <br>

  <%= u.submit "Upload Your Face!" %>

<% end %>
